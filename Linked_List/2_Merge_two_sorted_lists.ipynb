    "class Solution:\n",
    "    def reverseList(self, head: Optional[ListNode]) -> Optional[ListNode]:\n",
    "        q = None\n",
    "        r = None\n",
    "        p = head\n",
    "\n",
    "        while p is not None:\n",
    "            r = q\n",
    "            q = p\n",
    "            p = p.next\n",
    "            q.next = r\n",
    "\n",
    "        return q"
   ]
  }
